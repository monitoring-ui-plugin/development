[% INCLUDE mon_results_header.tt %]

<div class="td-mon-res-details-scroll">
  <table width="100%" class="mon-res-head-tbl" id="mon-res-tbl" cellspacing="0">
    <tbody style="">

[% FOREACH service IN hosts.keys.sort %]

  [% IF loop.index % 2 %]
      <tr class="mon-res-body-tr2" onClick="">
  [% ELSE %]
      <tr class="mon-res-body-tr" onClick="">
  [% END %]
        <td class="mon-res-body-td" colspan="1" width="25">
          <div style="outline:none;" id="mon-res-action-srv" status="[% hosts.$service.status %]" service="[% hosts.$service.service %]" host="[% name %]">
            <div>

        [% IF hosts.$service.status == 0 %]
	<img src="../share/images/icons/arrow-ok.png" />
        [% ELSIF hosts.$service.status == 1 %]
	<img src="../share/images/icons/arrow-warning.png" />
        [% ELSIF hosts.$service.status == 2 %]
	<img src="../share/images/icons/arrow-critical.png" />
        [% ELSIF hosts.$service.status == 3 %]
	<img src="../share/images/icons/arrow-unknown.png" />
        [% END %]

            </div>
          </div>
        </td>
        <td class="mon-res-body-td" width="170">
          <div style="outline:none;">
            <div id="mon-res-body-trim" style="overflow:hidden;">[% hosts.$service.service %]</div>
          </div>
        </td>
        <td class="mon-res-body-td">
          <div style="outline:none;">
            <div id="mon-res-body-trim" style="overflow:hidden;">[% hosts.$service.output %]</div>
          </div>
        </td>
      </tr>

[% END %]

    </tbody>
  </table>
  </div>
</div>

</div>

[% INCLUDE footer.tt %]
